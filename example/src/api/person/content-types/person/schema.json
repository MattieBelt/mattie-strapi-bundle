{
  "kind": "collectionType",
  "collectionName": "people",
  "info": {
    "singularName": "person",
    "pluralName": "people",
    "displayName": "Person",
    "name": "Person"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "hostedEpisodes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::episode.episode",
      "inversedBy": "hosts"
    },
    "ownedPodcasts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::podcast.podcast",
      "inversedBy": "owners"
    }
  }
}
